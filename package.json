{
  "name": "statiks",
  "version": "2.0.6",
  "private": true,
  "scripts": {
    "postinstall": "sh scripts/postinstall.sh",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint src",
    "version": "sh scripts/bump-ios.sh",
    "run-ios": "react-native run-ios --simulator='iPhone 8'",
    "run-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android"
  },
  "dependencies": {
    "@ueno/eslint-config": "1.2.7",
    "lodash": "4.17.4",
    "mobx": "3.2.1",
    "mobx-persist": "0.4.1",
    "mobx-react": "4.2.2",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-mixin": "4.0.0",
    "react-native": "0.51.0",
    "react-native-navigation": "1.1.295",
    "react-native-timer-mixin": "0.11.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "2.7.1",
    "babel-preset-react-native-stage-0": "1.0.1",
    "eslint": "4.9.0",
    "react-test-renderer": "16.0.0"
  },
  "moduleRoots": [
    "src/"
  ],
  "rnpm": {
    "assets": [
      "./src/assets/fonts"
    ]
  }
}
